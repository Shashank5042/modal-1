#include<stdio.h>
int main() 
{
    int i,j,sum=0,avg,rows=3,coloumns=3;
    int matrix[3][3]={{1,3,4}, {2,5,6},{7,8,9}};
        for (i=0;i<rows;i++) {
            for (j=0;j<coloumns;j++) {
                sum=sum + matrix[i][j];
             avg = sum/9;
            }
        }
        printf("the sum of the numbers is:%d",sum);
        printf("\n the average of the numbers is:%d",avg);
        return 0;
}
